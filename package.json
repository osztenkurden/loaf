{
  "name": "loaf",
  "version": "0.1.0",
  "homepage": "./",
  "description": "Loaf",
  "author": "Bakerysoft Montreal",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@peculiar/webcrypto": "^1.1.4",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.1",
    "@types/uuid": "^3.4.9",
    "big-integer": "^1.6.47",
    "bytebuffer": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "electron-builder-squirrel-windows": "^22.10.4",
    "fetch-cookie": "^0.7.3",
    "ip": "^1.1.5",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "notp": "^2.0.3",
    "qrcode": "^1.4.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "thirty-two": "github:osztenkurden/thirty-two#master",
    "tough-cookie": "^3.0.1",
    "typescript": "^4.0.5",
    "unused-filename": "^2.1.0",
    "util": "^0.12.3",
    "uuid": "^3.4.0"
  },
  "build": {
    "appId": "com.bakerysoft.loaf",
    "productName": "Loaf",
    "win": {
      "target": "NSIS"
    },
    "directories": {
      "output": "app",
      "buildResources": "assets"
    },
    "files": [
      "dist/**/*",
      "package.json",
      {
        "from": "dist",
        "to": "build",
        "filter": "**/*"
      },
      "!**/*.asar",
      "!**/app/**/*",
      "!**/app/*",
      "!**/win-unpacked/*",
      "!**/.git/**/*",
      "!**/.git/*"
    ]
  },
  "main": "dist/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run transpile && react-scripts build && rm -rf dist/build &&  mv build dist",
    "test": "react-scripts test",
    "transpile": "tsc electron.ts --outDir dist --esModuleInterop true",
    "electron": "npm run transpile && cross-env DEV=true electron ./",
    "electronDev1": "npm run transpile && cross-env DEV=true DEVUSER1=true electron ./",
    "electronDev2": "npm run transpile && cross-env DEV=true DEVUSER2=true electron ./",
    "electronDev3": "npm run transpile && cross-env DEV=true DEVUSER3=true electron ./",
    "dev": "concurrently \"cross-env BROWSER=none npm start\" \"npm run electron\"",
    "eject": "react-scripts eject",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "compile": "npm run build && npm run pack"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.42",
    "@types/cors": "^2.8.9",
    "@types/material-ui": "^0.21.8",
    "@types/node": "^12.19.11",
    "@types/node-fetch": "^2.5.7",
    "@types/qrcode": "^1.3.5",
    "@types/react-router-dom": "^5.1.6",
    "@types/request": "^2.48.5",
    "@types/socket.io": "^2.1.12",
    "@types/socket.io-client": "^1.4.34",
    "@types/sqlite3": "^3.1.6",
    "@types/tough-cookie": "^2.3.7",
    "concurrently": "^5.3.0",
    "electron": "^11.1.1",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.11.0",
    "wait-on": "^5.2.1"
  }
}
