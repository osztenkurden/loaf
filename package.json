{
  "name": "loaf",
  "version": "0.1.0",
  "homepage": "./",
  "description": "Loaf",
  "author": "Bakerysoft Montreal",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@peculiar/webcrypto": "^1.0.27",
    "big-integer": "^1.6.47",
    "bytebuffer": "^5.0.1",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "fetch-cookie": "^1.0.0",
    "ip": "^1.1.5",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.1",
    "notp": "^2.0.3",
    "qrcode": "^1.4.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-intersection-observer": "^8.32.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "sequelize": "^6.6.5",
    "socket.io-client": "^4.4.1",
    "sqlite3": "^5.0.2",
    "thirty-two": "github:osztenkurden/thirty-two#master",
    "tough-cookie": "^4.0.0",
    "tough-cookie-file-store": "^2.0.3",
    "typescript": "^4.2.3",
    "unused-filename": "^2.1.0",
    "util": "^0.12.1",
    "uuid": "^8.3.2"
  },
  "build": {
    "appId": "com.bakerysoft.loaf",
    "productName": "Loaf",
    "win": {
      "target": "NSIS"
    },
    "directories": {
      "output": "app",
      "buildResources": "assets"
    },
    "files": [
      "dist/**/*",
      "package.json",
      {
        "from": "dist",
        "to": "build",
        "filter": "**/*"
      },
      "!**/*.asar",
      "!**/app/**/*",
      "!**/app/*",
      "!**/win-unpacked/*",
      "!**/.git/**/*",
      "!**/.git/*"
    ]
  },
  "main": "dist/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run transpile && react-scripts build && rm -rf dist/build &&  mv build dist",
    "test": "react-scripts test",
    "transpile": "tsc -p tsconfig.electron.json",
    "electron": "npm run transpile && cross-env DEV=true electron ./",
    "electronDist": "npm run transpile && electron ./",
    "electronLocal": "npm run transpile && cross-env DEV=true local=true electron ./",
    "electronDev1": "npm run transpile && cross-env DEV=true DEVUSER1=true electron ./",
    "electronDev2": "npm run transpile && cross-env DEV=true DEVUSER2=true electron ./",
    "electronDev3": "npm run transpile && cross-env DEV=true DEVUSER3=true electron ./",
    "dev": "concurrently \"cross-env BROWSER=none npm start\" \"npm run electron\"",
    "eject": "react-scripts eject",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "onlyCompile": "npm run transpile && npm run pack",
    "compile": "npm run build && npm run pack"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.40",
    "@types/cors": "^2.8.6",
    "@types/jest": "24.0.18",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.6.1",
    "@types/qrcode": "^1.3.4",
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.1",
    "@types/react-infinite-scroller": "^1.2.2",
    "@types/react-router-dom": "^5.1.3",
    "@types/request": "^2.48.3",
    "@types/sqlite3": "^3.1.6",
    "@types/tough-cookie": "^2.3.6",
    "@types/tough-cookie-file-store": "^2.0.1",
    "@types/uuid": "^3.4.6",
    "concurrently": "^5.3.0",
    "electron": "^17.1.1",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.6",
    "wait-on": "^5.2.0"
  }
}
